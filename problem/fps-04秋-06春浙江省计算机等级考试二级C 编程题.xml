<test_input><![CDATA[1]]></test_input>
<test_output><![CDATA[1.41]]></test_output>
<hint><![CDATA[]]></hint>
<source><![CDATA[]]></source>
<solution language="C"><![CDATA[#include <stdio.h>
#include <math.h>

int main()
{
	double x,res;
	scanf("%lf",&x);
	if(x<0)
		res = fabs(x);
	else if(0<=x && x<2)
		res = sqrt(x+1);
	else if(2<=x && x<4)
		res = pow(x+2,5.0);
	else
		res = 2*x+5;
	printf("%.2lf\n",res);

	return 0;
}]]></solution>
