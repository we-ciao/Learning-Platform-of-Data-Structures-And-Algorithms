<solution language="C"><![CDATA[#include<stdio.h>

int main()
{
	int high=0,mid=0,low=0;
	int x;
	while(scanf("%d",&x), x>0)
	{
		if(x >= 85)
			high++;
		else if(x >= 60)
			mid++;
		else
			low++;
	}
	printf(">=85:%d\n60-84:%d\n<60:%d\n",high,mid,low);

	return 0;
}]]></solution>
